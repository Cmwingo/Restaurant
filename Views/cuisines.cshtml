<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Cuisines</title>
  </head>
  <body>
    <h1>All Cuisines</h1>
    @if (@Model.Count == 0)
    {
      <p>There are no cuisines.</p>
    }
    <ul>
      @foreach(var cuisine in Model)
      {
        <li><a href="/cuisines/@cuisine.GetId()">@cuisine.GetName()</a></li>
      }
    </ul>
    <a href="/cuisines/new">Add a new cuisine</a>
  </body>
</html>
